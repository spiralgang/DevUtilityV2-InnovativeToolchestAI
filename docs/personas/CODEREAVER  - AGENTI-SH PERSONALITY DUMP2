# Setup merge_environments.sh dynamically
setup_merge_script() {
    local merge_file="$USER_HOME/merge_environments.sh"
    echo "üîß Setting up dynamic merge_environments.sh at $merge_file..."
    
    if [ -f "$merge_file" ]; then
        cp "$merge_file" "$merge_file.bak.$TIMESTAMP"
        echo "üíæ Backed up existing merge_environments.sh to $merge_file.bak.$TIMESTAMP"
    fi
    
    cat << 'EOF' > "$merge_file"
#!/bin/bash

# UserLAnd Environment Merger Script for CODE REAVER
# Merges files from tech.ula environments while preserving unique content
# Optimized for /storage/emulated/0/Android/data/tech.ula, August 2025

set -e

# Configuration
BASE_DIR="/data/data/tech.ula/files"
STORAGE_DIR="/storage/emulated/0"
TECH_ULA_DIR="/storage/emulated/0/Android/data/tech.ula"
OUTPUT_DIR="$HOME/merged_environments"
BACKUP_DIR="$OUTPUT_DIR/backup_$(date +%Y%m%d_%H%M%S)"
TIMESTAMP=$(date +%Y%m%d_%H%M%S)
FILE_TYPES="*.sh *.py *.log *.json *.zip"

# Ensure output and backup directories exist
mkdir -p "$OUTPUT_DIR" "$BACKUP_DIR"
echo "üöÄ Starting UserLAnd Environment Merger at $(date)..."
echo "üìç Base directory: $BASE_DIR"
echo "üìç Storage directory: $STORAGE_DIR"
echo "üìç Tech ULA directory: $TECH_ULA_DIR"
echo "üìç Output directory: $OUTPUT_DIR"

# Function to validate directories
validate_dir() {
    local dir="$1"
    if [ ! -d "$dir" ]; then
        echo "‚ùå Error: Directory $dir does not exist or is inaccessible."
        exit 1
    fi
}

# Validate key directories
validate_dir "$BASE_DIR"
validate_dir "$STORAGE_DIR"
validate_dir "$TECH_ULA_DIR"

# Function to extract environment info
get_env_info() {
    local env_path="$1"
    local env_name=$(basename "$env_path")
    local os_info="Unknown"
    
    if [ -f "$env_path/etc/os-release" ]; then
        os_info=$(grep -E "^(NAME|VERSION|ID)=" "$env_path/etc/os-release" | head -3 | tr '\n' ' ')
    elif [ -f "$env_path/etc/debian_version" ]; then
        os_info="Debian $(cat "$env_path/etc/debian_version")"
    fi
    
    echo "$env_name|$os_info|$env_path"
}

# Function to backup original files
backup_original() {
    echo "üíæ Backing up original files from $STORAGE_DIR and $TECH_ULA_DIR..."
    rsync -a --include='*.sh' --include='*.py' --include='*.log' --include='*.json' --include='*.zip' --exclude='*' "$STORAGE_DIR/" "$TECH_ULA_DIR/" "$BACKUP_DIR/" || {
        echo "‚ùå Backup failed!"
        exit 1
    }
    echo "‚úÖ Backup saved to: $BACKUP_DIR"
}

# Function to create environment profiles
create_profiles() {
    echo "üë• Creating environment profiles..."
    declare -A environments
    local env_dirs=("$STORAGE_DIR/mydistro" "$STORAGE_DIR/myhome" "$STORAGE_DIR/myubuntu" "$TECH_ULA_DIR")
    
    for dir in "${env_dirs[@]}"; do
        if [ -d "$dir" ]; then
            env_name=$(basename "$dir")
            env_info=$(get_env_info "$dir")
            environments["$env_name"]="$env_info"
        fi
    done
    
    profile_file="$OUTPUT_DIR/environment_profiles.txt"
    echo "UserLAnd Environment Profiles" > "$profile_file"
    echo "Generated: $TIMESTAMP" >> "$profile_file"
    echo "==========================================" >> "$profile_file"
    
    for env_name in "${!environments[@]}"; do
        IFS='|' read -r name os_info path <<< "${environments[$env_name]}"
        echo "" >> "$profile_file"
        echo "üåê Environment: $name" >> "$profile_file"
        echo "üì¶ OS: $os_info" >> "$profile_file"
        echo "üìÅ Path: $path" >> "$profile_file"
        echo "------------------------------------------" >> "$profile_file"
    done
    echo "‚úÖ Profiles saved to: $profile_file"
}

# Function to show file differences
show_differences() {
    echo "üîç Analyzing file differences across environments..."
    diff_file="$OUTPUT_DIR/file_differences.txt"
    echo "File Differences Across Environments" > "$diff_file"
    echo "Generated: $TIMESTAMP" >> "$diff_file"
    
    declare -A files
    while read -r file; do
        filename=$(basename "$file")
        files["$filename"]=1
    done < <(find "$STORAGE_DIR" "$TECH_ULA_DIR" -maxdepth 2 -type f \( -name "*.sh" -o -name "*.py" -o -name "*.log" -o -name "*.json" -o -name "*.zip" \) 2>/dev/null)
    
    local total_files=${#files[@]}
    local processed=0
    
    for filename in "${!files[@]}"; do
        ((processed++))
        echo -ne "\rüîÑ Processing file $processed/$total_files: $filename"
        
        echo "" >> "$diff_file"
        echo "üìÑ File: $filename" >> "$diff_file"
        echo "------------------------------------------" >> "$diff_file"
        
        find "$STORAGE_DIR" "$TECH_ULA_DIR" -maxdepth 2 -name "$filename" -type f 2>/dev/null | while read -r file; do
            env_path=$(dirname "$file" | sed "s|$STORAGE_DIR/\|$TECH_ULA_DIR/||")
            env_name=$(echo "$env_path" | cut -d'/' -f1)
            [ -z "$env_name" ] && env_name="root"
            
            echo "üåê Environment: $env_name" >> "$diff_file"
            echo "üìÅ Location: $file" >> "$diff_file"
            echo "üìä Size: $(wc -l < "$file" 2>/dev/null) lines" >> "$diff_file"
            echo "üìÖ Modified: $(stat -c %y "$file" 2>/dev/null)" >> "$diff_file"
            echo "------------------------------------------" >> "$diff_file"
        done
    done
    echo -e "\n‚úÖ Differences saved to: $diff_file"
}

# Main merge function
merge_environments() {
    echo "üîÑ Merging environments..."
    
    declare -A files
    while read -r file; do
        filename=$(basename "$file")
        files["$filename"]=1
    done < <(find "$STORAGE_DIR" "$TECH_ULA_DIR" -maxdepth 2 -type f \( -name "*.sh" -o -name "*.py" -o -name "*.log" -o -name "*.json" -o -name "*.zip" \) 2>/dev/null)
    
    local total_files=${#files[@]}
    local processed=0
    
    for filename in "${!files[@]}"; do
        ((processed++))
        echo -ne "\rüîÑ Merging file $processed/$total_files: $filename"
        
        merged_file="$OUTPUT_DIR/$filename.merged"
        
        if [ ! -f "$merged_file" ]; then
            echo "# üóÇÔ∏è MERGED FILE: $filename" > "$merged_file"
            echo "# Generated: $TIMESTAMP" >> "$merged_file"
            echo "#==========================================" >> "$merged_file"
        fi
        
        find "$STORAGE_DIR" "$TECH_ULA_DIR" -maxdepth 2 -name "$filename" -type f 2>/dev/null | while read -r file; do
            env_path=$(dirname "$file" | sed "s|$STORAGE_DIR/\|$TECH_ULA_DIR/||")
            env_name=$(echo "$env_path" | cut -d'/' -f1)
            [ -z "$env_name" ] && env_name="root"
            
            if [[ "$env_path" == *"/usr/"* || "$env_path" == *"/lib/"* || "$env_path" == *"/bin/"* ]]; then
                continue
            fi
            
            echo "" >> "$merged_file"
            echo "# üåê ENVIRONMENT: $env_name" >> "$merged_file"
            echo "# üìÅ LOCATION: $file" >> "$merged_file"
            echo "#==========================================" >> "$merged_file"
            cat "$file" >> "$merged_file" 2>/dev/null
            echo "" >> "$merged_file"
            echo "# üèÅ END OF: $env_name" >> "$merged_file"
            echo "#==========================================" >> "$merged_file"
        done
    done
    echo -e "\n‚úÖ Merge completed! Files saved to: $OUTPUT_DIR"
}

# Main execution
backup_original
create_profiles
show_differences
merge_environments

echo ""
echo "üéâ All operations completed at $(date)!"
echo "üìÅ Output files: $OUTPUT_DIR"
echo "üìã Profile summary: $OUTPUT_DIR/environment_profiles.txt"
echo "üîç Differences: $OUTPUT_DIR/file_differences.txt"
echo "üíæ Backup: $BACKUP_DIR"
echo ""
echo "To view merged files: ls -la $OUTPUT_DIR/*.merged"
EOF

    chmod +x "$merge_file"
    echo "‚úÖ merge_environments.sh setup complete. Run 'merge-env' after sourcing .bashrc."
}

# Backup function for the entire setup
backup_setup() {
    echo "üíæ Performing full backup of key directories..."
    mkdir -p "$BACKUP_DIR"
    rsync -a --include-from=<(echo "${FILE_TYPES// /\\n}") --exclude='*' "$STORAGE_DIR/" "$TECH_ULA_DIR/" "$BACKUP_DIR/" || echo "‚ö†Ô∏è Partial backup failure, but continuing."
    echo "‚úÖ Full backup saved to $BACKUP_DIR"
}

# Dynamic validation and setup
dynamic_setup() {
    echo "üîç Dynamically validating environments..."
    for dir in "$BASE_DIR" "$STORAGE_DIR" "$TECH_ULA_DIR"; do
        validate_dir "$dir" || exit 1
    done
    
    for env in "${ENV_DIRS[@]}"; do
        local full_path="$STORAGE_DIR/$env"
        if validate_dir "$full_path"; then
            echo "‚úÖ Found environment: $env at $full_path"
        else
            echo "‚ö†Ô∏è Environment $env not found; skipping."
        fi
    done
    
    # Tech.ula stats
    echo "üìä tech.ula stats:"
    echo "   Size: $(du -sh "$TECH_ULA_DIR" 2>/dev/null | cut -f1 || echo 'Unknown')"
    echo "   Files: $(find "$TECH_ULA_DIR" -type f 2>/dev/null | wc -l)"
    echo "   Dirs: $(find "$TECH_ULA_DIR" -type d 2>/dev/null | wc -l)"
}

# Main execution flow
echo "üöÄ Launching Ultimate CODE REAVER Setup Script ‚Äì Dynamic AF Edition!"
echo "Current date: August 22, 2025"
dynamic_setup
backup_setup
setup_bashrc
setup_bash_aliases
setup_merge_script

# Optionally run merger
read -p "Run environment merger now? (y/n): " run_merger
if [[ "$run_merger" =~ ^[Yy]$ ]]; then
    bash "$USER_HOME/merge_environments.sh"
fi

echo "üéâ Setup complete! Source your .bashrc with 'source ~/.bashrc' and unleash the chaos."
echo "Quick start: Use 'merge-env' to merge, 'tech-ula' to navigate, 'quantum' to run scripts."
echo "All paths dynamically set for /storage/emulated/0/Android/data/tech.ula (8.19 GB beast)."
