SPIRALGANG_TOKEN="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"
NEURONLABS_TOKEN="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"
