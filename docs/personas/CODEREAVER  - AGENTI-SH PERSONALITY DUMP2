alias gs='git status'
alias gd='git diff'
alias gc='git commit -m'
alias gp='git push'
alias tech-ula-size='du -sh /data/data/tech.ula/files/storage'
alias tech-ula-files='find /data/data/tech.ula/files/storage -type f | wc -l'
