echo "üåê GitHub repos: $OUTPUT_DIR/spiralgang, $OUTPUT_DIR/neuronlabs"
echo "üîó APK symlink: $APK_SYMLINK"
echo ""
echo "To view merged files: ls -la $OUTPUT_DIR/*.merged"
EOF

    chmod +x "$merge_file"
    echo "‚úÖ merge_environments.sh setup complete with gh CLI and symlink integration."
}

# Backup function
backup_setup() {
    echo "üíæ Performing full backup of key directories..."
    mkdir -p "$BACKUP_DIR"
    rsync -a --include-from=<(echo "${FILE_TYPES// /\\n}") --exclude='*' "$STORAGE_DIR/" "$TECH_ULA_DIR/" "$BACKUP_DIR/" || echo "‚ö†Ô∏è Partial backup failure, but continuing."
    echo "‚úÖ Full backup saved to $BACKUP_DIR"
}

# Dynamic validation and setup
dynamic_setup() {
    echo "üîç Dynamically validating environments..."
    for dir in "$BASE_DIR" "$STORAGE_DIR" "$TECH_ULA_DIR"; do
        validate_dir "$dir" || exit 1
    done
    
    for env in "${ENV_DIRS[@]}"; do
        local full_path="$STORAGE_DIR/$env"
        if validate_dir "$full_path"; then
            echo "‚úÖ Found environment: $env at $full_path"
        else
            echo "‚ö†Ô∏è Environment $env not found; skipping."
        fi
    done
    
    echo "üìä tech.ula stats (from $TECH_ULA_DIR):"
    echo "   Size: $(du -sh "$TECH_ULA_DIR" 2>/dev/null | cut -f1 || echo 'Unknown')"
    echo "   Files: $(find "$TECH_ULA_DIR" -type f 2>/dev/null | wc -l)"
    echo "   Dirs: $(find "$TECH_ULA_DIR" -type d 2>/dev/null | wc -l)"
}

