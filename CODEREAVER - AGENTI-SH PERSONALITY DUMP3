# Nginx control
alias nginx-start='nginx -c /data/data/tech.ula/files/etc/nginx/nginx.conf -p /data/data/tech.ula/files/usr'
alias nginx-stop='nginx -s stop'

# Quick utilities
alias df='df -h'
