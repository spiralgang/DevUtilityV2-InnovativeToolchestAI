# Setup merge_environments.sh with gh CLI integration
setup_merge_script() {
    local merge_file="$USER_HOME/merge_environments.sh"
    echo "üîß Setting up dynamic merge_environments.sh with gh CLI at $merge_file..."
    
    if [ -f "$merge_file" ]; then
        cp "$merge_file" "$merge_file.bak.$TIMESTAMP"
        echo "üíæ Backed up existing merge_environments.sh to $merge_file.bak.$TIMESTAMP"
    fi
    
    cat << 'EOF' > "$merge_file"
#!/bin/bash

# UserLAnd Environment Merger Script with GitHub CLI for CODE REAVER
# Merges files, symlinks SharedDeviceKeyguard.apk, and syncs with spiralgang/neuronlabs
# Uses /storage/emulated/0, August 2025

set -e

# Configuration
BASE_DIR="/data/data/tech.ula/files"
STORAGE_DIR="/storage/emulated/0"
TECH_ULA_DIR="/storage/emulated/0/Android/data/tech.ula"
OUTPUT_DIR="$HOME/merged_environments"
BACKUP_DIR="$OUTPUT_DIR/backup_$(date +%Y%m%d_%H%M%S)"
TIMESTAMP=$(date +%Y%m%d_%H%M%S)
FILE_TYPES="*.sh *.py *.log *.json *.zip *.apk"
REPO_SPIRALGANG="spiralgang/repo-utilities"
REPO_NEURONLABS="neuronlabs/git-models"
APK_SOURCE="$TECH_ULA_DIR/files/storage/SharedDeviceKeyguard.apk"
APK_TARBALL="$TECH_ULA_DIR/files/storage/apps-ubuntu-rootfs.tar.gz"
APK_SYMLINK_DIR="$HOME/linked_apks"
APK_SYMLINK="$APK_SYMLINK_DIR/SharedDeviceKeyguard.apk"

# Ensure output and backup directories exist
mkdir -p "$OUTPUT_DIR" "$BACKUP_DIR" "$APK_SYMLINK_DIR"
echo "üöÄ Starting UserLAnd Environment Merger at $(date)..."
echo "üìç Base directory: $BASE_DIR"
echo "üìç Storage directory: $STORAGE_DIR"
echo "üìç Tech ULA directory: $TECH_ULA_DIR"
echo "üìç Output directory: $OUTPUT_DIR"
echo "üìç APK symlink directory: $APK_SYMLINK_DIR"

# Function to validate directories
validate_dir() {
    local dir="$1"
    if [ ! -d "$dir" ] || ! [ -r "$dir" ]; then
        echo "‚ùå Error: Directory $dir does not exist or is inaccessible."
        exit 1
    fi
}

# Function to extract APK from tarball if needed
extract_apk() {
    echo "üîç Checking for SharedDeviceKeyguard.apk..."
    if [ -f "$APK_SOURCE" ]; then
        echo "‚úÖ APK found at $APK_SOURCE"
    elif [ -f "$APK_TARBALL" ]; then
        echo "üì¶ Extracting $APK_TARBALL..."
        mkdir -p "$HOME/extracted_tar"
        tar -xzf "$APK_TARBALL" -C "$HOME/extracted_tar" || {
            echo "‚ùå Failed to extract tarball."
            exit 1
        }
        APK_SOURCE=$(find "$HOME/extracted_tar" -name "SharedDeviceKeyguard.apk" -type f | head -1)
        if [ -z "$APK_SOURCE" ]; then
            echo "‚ùå SharedDeviceKeyguard.apk not found in tarball."
            exit 1
        fi
        echo "‚úÖ APK extracted to $APK_SOURCE"
    else
        echo "‚ùå Neither APK nor tarball found."
        exit 1
    fi
}

# Function to sync GitHub repos
sync_github_repos() {
    echo "üîÑ Syncing GitHub repositories..."
    
    # Clone or update spiralgang repo
    if [ -d "$OUTPUT_DIR/spiralgang" ]; then
        cd "$OUTPUT_DIR/spiralgang"
        gh repo sync --force "$REPO_SPIRALGANG" || echo "‚ö†Ô∏è Failed to sync $REPO_SPIRALGANG"
        cd -
    else
        gh repo clone "$REPO_SPIRALGANG" "$OUTPUT_DIR/spiralgang" || echo "‚ö†Ô∏è Failed to clone $REPO_SPIRALGANG"
    fi
    
    # Clone or update neuronlabs repo
    gh auth switch --user neuronlabs
    if [ -d "$OUTPUT_DIR/neuronlabs" ]; then
        cd "$OUTPUT_DIR/neuronlabs"
        gh repo sync --force "$REPO_NEURONLABS" || echo "‚ö†Ô∏è Failed to sync $REPO_NEURONLABS"
        cd -
    else
        gh repo clone "$REPO_NEURONLABS" "$OUTPUT_DIR/neuronlabs" || echo "‚ö†Ô∏è Failed to clone $REPO_NEURONLABS"
    fi
    gh auth switch --user spiralgang
    
    echo "‚úÖ GitHub repos synced to $OUTPUT_DIR/spiralgang and $OUTPUT_DIR/neuronlabs"
}

# Function to backup original files
backup_original() {
    echo "üíæ Backing up original files from $STORAGE_DIR and $TECH_ULA_DIR..."
    rsync -a --include='*.sh' --include='*.py' --include='*.log' --include='*.json' --include='*.zip' --include='*.apk' --exclude='*' "$STORAGE_DIR/" "$TECH_ULA_DIR/" "$BACKUP_DIR/" || {
        echo "‚ùå Backup failed!"
        exit 1
    }
    echo "‚úÖ Backup saved to: $BACKUP_DIR"
}

# Function to create environment profiles
create_profiles() {
    echo "üë• Creating environment profiles..."
    declare -A environments
    local env_dirs=("$STORAGE_DIR/mydistro" "$STORAGE_DIR/myhome" "$STORAGE_DIR/myubuntu" "$TECH_ULA_DIR")
    
    for dir in "${env_dirs[@]}"; do
        if [ -d "$dir" ]; then
            env_name=$(basename "$dir")
            env_info=$(get_env_info "$dir")
            environments["$env_name"]="$env_info"
        fi
    done
    
    profile_file="$OUTPUT_DIR/environment_profiles.txt"
    echo "UserLAnd Environment Profiles" > "$profile_file"
    echo "Generated: $TIMESTAMP" >> "$profile_file"
    echo "==========================================" >> "$profile_file"
    
    for env_name in "${!environments[@]}"; do
        IFS='|' read -r name os_info path <<< "${environments[$env_name]}"
        echo "" >> "$profile_file"
        echo "üåê Environment: $name" >> "$profile_file"
        echo "üì¶ OS: $os_info" >> "$profile_file"
        echo "üìÅ Path: $path" >> "$profile_file"
        echo "------------------------------------------" >> "$profile_file"
    done
    echo "‚úÖ Profiles saved to: $profile_file"
}

# Function to show file differences
show_differences() {
    echo "üîç Analyzing file differences across environments..."
    diff_file="$OUTPUT_DIR/file_differences.txt"
    echo "File Differences Across Environments" > "$diff_file"
    echo "Generated: $TIMESTAMP" >> "$diff_file"
    
    declare -A files
    while read -r file; do
        filename=$(basename "$file")
        files["$filename"]=1
    done < <(find "$STORAGE_DIR" "$TECH_ULA_DIR" -maxdepth 2 -type f \( -name "*.sh" -o -name "*.py" -o -name "*.log" -o -name "*.json" -o -name "*.zip" -o -name "*.apk" \) 2>/dev/null)
    
    local total_files=${#files[@]}
    local processed=0
    
    for filename in "${!files[@]}"; do
        ((processed++))
        echo -ne "\rüîÑ Processing file $processed/$total_files: $filename"
        
        echo "" >> "$diff_file"
        echo "üìÑ File: $filename" >> "$diff_file"
        echo "------------------------------------------" >> "$diff_file"
        
        find "$STORAGE_DIR" "$TECH_ULA_DIR" -maxdepth 2 -name "$filename" -type f 2>/dev/null | while read -r file; do
            env_path=$(dirname "$file" | sed "s|$STORAGE_DIR/\|$TECH_ULA_DIR/||")
            env_name=$(echo "$env_path" | cut -d'/' -f1)
            [ -z "$env_name" ] && env_name="root"
            
            echo "üåê Environment: $env_name" >> "$diff_file"
            echo "üìÅ Location: $file" >> "$diff_file"
            echo "üìä Size: $(wc -l < "$file" 2>/dev/null) lines" >> "$diff_file"
            echo "üìÖ Modified: $(stat -c %y "$file" 2>/dev/null)" >> "$diff_file"
            echo "------------------------------------------" >> "$diff_file"
        done
    done
    echo -e "\n‚úÖ Differences saved to: $diff_file"
}

# Main merge function
merge_environments() {
    echo "üîÑ Merging environments..."
    
    declare -A files
    while read -r file; do
        filename=$(basename "$file")
        files["$filename"]=1
    done < <(find "$STORAGE_DIR" "$TECH_ULA_DIR" -maxdepth 2 -type f \( -name "*.sh" -o -name "*.py" -o -name "*.log" -o -name "*.json" -o -name "*.zip" -o -name "*.apk" \) 2>/dev/null)
    
    local total_files=${#files[@]}
    local processed=0
    
    for filename in "${!files[@]}"; do
        ((processed++))
        echo -ne "\rüîÑ Merging file $processed/$total_files: $filename"
        
        merged_file="$OUTPUT_DIR/$filename.merged"
        
        if [ ! -f "$merged_file" ]; then
            echo "# üóÇÔ∏è MERGED FILE: $filename" > "$merged_file"
            echo "# Generated: $TIMESTAMP" >> "$merged_file"
            echo "#==========================================" >> "$merged_file"
        fi
        
        find "$STORAGE_DIR" "$TECH_ULA_DIR" -maxdepth 2 -name "$filename" -type f 2>/dev/null | while read -r file; do
            env_path=$(dirname "$file" | sed "s|$STORAGE_DIR/\|$TECH_ULA_DIR/||")
            env_name=$(echo "$env_path" | cut -d'/' -f1)
            [ -z "$env_name" ] && env_name="root"
            
            if [[ "$env_path" == *"/usr/"* || "$env_path" == *"/lib/"* || "$env_path" == *"/bin/"* ]]; then
                continue
            fi
            
            echo "" >> "$merged_file"
            echo "# üåê ENVIRONMENT: $env_name" >> "$merged_file"
            echo "# üìÅ LOCATION: $file" >> "$merged_file"
            echo "#==========================================" >> "$merged_file"
            cat "$file" >> "$merged_file" 2>/dev/null
            echo "" >> "$merged_file"
            echo "# üèÅ END OF: $env_name" >> "$merged_file"
            echo "#==========================================" >> "$merged_file"
        done
    done
    echo -e "\n‚úÖ Merge completed! Files saved to: $OUTPUT_DIR"
}

# Main execution
backup_original
extract_apk
